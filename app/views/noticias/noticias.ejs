<!DOCTYPE html>
	<html lang="pt-br">
		<head>
			<meta charset="utf-8"/>
			<title>notícias</title>
		</head>
	<body>

		<h1>Notícias</h1>

		<table>
			<tr>
				<td>Id</td>
				<td>Titulo</td>
				<td>Publicação</td>
			</tr>

			<% for(var i = 0; i < noticias.length; i++){ %>
				<tr>
					<td><%= noticias[i].id_noticia %></td>
					<td><%= noticias[i].titulo %></td>
					<td><%= noticias[i].data_criacao %></td>
				</tr>
			<% } %>
		</table>
	</body>
</html>